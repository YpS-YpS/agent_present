# Claude API Key (leave empty and set USE_MOCK=true for development without API key)
ANTHROPIC_API_KEY=sk-ant-...

# Set to true to use mock LLM mode (no API key needed)
USE_MOCK=true

# Claude model to use
MODEL_NAME=claude-sonnet-4-20250514

# Max tokens per response
MAX_TOKENS=4096

# File upload limits
MAX_FILE_SIZE_MB=100
MAX_ROWS_PER_FILE=500000

# Frontend URL for CORS
CORS_ORIGINS=["http://localhost:3000"]
